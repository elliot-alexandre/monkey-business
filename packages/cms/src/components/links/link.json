      "regex": "^(http:\\/\\/www\\.|https:\\/\\/www\\.|http:\\/\\/|https:\\/\\/)?[a-z0-9]+([\\-\\.]{1}[a-z0-9]+)*\\.[a-z]{2,5}(:[0-9]{1,5})?(\\/.*)?$"
    },
    "Page": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "Section": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::section.section"
